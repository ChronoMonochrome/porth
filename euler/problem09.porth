include "std.porth"

macro a mem end
macro b a 8 + end
macro c b 8 + end

1 while dup 1000 < do
  dup a !64
  1 while dup a @64 + 1000 < do
    dup b !64
    1000 a @64 - b @64 - c !64
        
    if a @64 dup * b @64 dup * + c @64 dup * = do
      a @64 b @64 * c @64 * print
      0 exit
    end
      
    1 +
  end drop
  1 +
end drop

here eputs ": unreachable\n" eputs
1 exit
